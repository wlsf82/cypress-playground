{
  "name": "cypress-playground",
  "version": "1.0.0",
  "description": "Cypress Playground app and tests.",
  "main": "src/index.html",
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:prod": "cypress open --env environment=prod",
    "test": "cypress run --record false --browser chrome",
    "test:prod": "cypress run --record false --browser chrome --env environment=prod",
    "test:prod:cloud": "cypress run --browser chrome --record --tag 'prod' --env environment=prod",
    "pw:open": "playwright test --ui"
  },
  "keywords": [
    "Cypress Playground",
    "Test Design Masterclass TAT",
    "Talking About Testing",
    "Cypress Testing",
    "Testing"
  ],
  "author": "Walmyr <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.2",
    "axe-core": "^4.10.2",
    "cypress": "^13.17.0",
    "cypress-axe": "^1.5.0",
    "lodash": "^4.17.21"
  }
}
